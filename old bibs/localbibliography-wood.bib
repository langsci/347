@article{Meyer2010,
	Abstract = {Reflexive passives and impersonals are present in all modern Slavonic languages, but vary in their synchronic properties. The present paper analyses the diachronic developments that lead to this variation in Czech, Polish, and Russian and includes some background on Old Church Slavonic. It is shown how the former three languages reached their differing positions in a typological hierarchy of passive constructions, by stepwise reanalysis. The diachronic comparative approach furthermore demonstrates the interdependence/independence of some of the empirical features of reflexive passives and impersonals in Slavonic. (English) [ABSTRACT FROM AUTHOR]},
	Author = {Meyer, Roland},
